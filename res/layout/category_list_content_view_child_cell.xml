<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FAFAFA"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/LinearLayout_CategoryListContentViewChildCell_Total"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FAFAFA"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:visibility="gone" >

            <View
                android:id="@+id/View_TopLine_CategoryListContentViewChildCell_Total"
                android:layout_width="match_parent"
                android:layout_height="35dp" />

            <TextView
                android:id="@+id/TextView_CategoryListContentViewChildCell_Total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.5dp"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:includeFontPadding="false"
                android:lines="1"
                android:text="buttonall"
                android:textColor="#6D6F79"
                android:textSize="16dp" />

            <View
                android:id="@+id/View_MiddleLine_CategoryListContentViewChildCell_Total"
                android:layout_width="match_parent"
                android:layout_height="34dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout_CategoryListContentViewChildCell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FAFAFA"
            android:gravity="center_vertical"
            android:orientation="vertical" >

            <View
                android:id="@+id/View_TopLine_CategoryListContentViewChildCell"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/TextView_CategoryListContentViewChildCell"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50.5dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:lines="1"
                    android:textColor="#6D6F79"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/TextView_CategoryListContentViewChildCell_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:includeFontPadding="false"
                    android:lines="1"
                    android:paddingLeft="10dp"
                    android:textColor="#90959E"
                    android:textSize="13dp" />
            </LinearLayout>

            <View
                android:id="@+id/View_MiddleLine_CategoryListContentViewChildCell"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:visibility="visible" />

            <View
                android:id="@+id/View_BottomLine_CategoryListContentViewChildCell"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="gone" />
        </LinearLayout>
    </LinearLayout>

</merge>